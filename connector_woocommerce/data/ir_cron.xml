<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="ir_cron_woocommerce_export_product" model="ir.cron" forcecreate="True">
        <field name="name">WooCommerce - Export products</field>
        <field name="model_id" ref="model_woocommerce_backend" />
        <field name="active" eval="False" />
        <!-- Mandatory to define a explicit user on record from the frontend,
             DON'T USE admin (base.user_root) -->
        <!--field name="user_id" ref="<not base.user_root>"/-->
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="state">code</field>
        <field name="code">model._scheduler_export_products()</field>
    </record>
</odoo>
