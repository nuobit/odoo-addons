<odoo noupdate="1">
    <!--   Export Product product-->
    <record
        id="woocommerce_product_product_export_batch_job_function"
        model="queue.job.function"
    >
        <field
            name="model_id"
            ref="connector_woocommerce.model_woocommerce_product_product"
        />
        <field name="method">export_batch</field>
        <field
            name="channel_id"
            ref="connector_woocommerce.woocommerce_export_record"
        />
        <field name="retry_pattern" eval="{1: 10, 5: 30, 10: 60, 15: 300}" />
