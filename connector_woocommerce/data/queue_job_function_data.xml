<odoo noupdate="1">
    <!--    Product product-->
    <record
        id="woocommerce_product_product_export_batch_job_function"
        model="queue.job.function"
    >
        <field
            name="model_id"
            ref="connector_woocommerce.model_woocommerce_product_product"
        />
        <field name="method">export_batch</field>
        <field name="channel_id" ref="connector_woocommerce.channel_woocommerce" />
        <field name="retry_pattern" eval="{1: 10, 5: 30, 10: 60, 15: 300}" />
    </record>
    <record
        id="woocommerce_product_product_export_record_job_function"
        model="queue.job.function"
    >
        <field
            name="model_id"
            ref="connector_woocommerce.model_woocommerce_product_product"
        />
        <field name="method">export_record</field>
        <field name="channel_id" ref="connector_woocommerce.channel_woocommerce" />
        <field name="retry_pattern" eval="{1: 10, 5: 30, 10: 60, 15: 300}" />
    </record>
</odoo>
