<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="delivery_extended_delivery_carrier_form_view" model="ir.ui.view">
        <field name="name">delivery.extended.delivery.carrier.form.inherit</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='fixed_price']" position="after">
                <field name="fixed_price_without_taxes"/>
            </xpath>
        </field>
    </record>
</odoo>
