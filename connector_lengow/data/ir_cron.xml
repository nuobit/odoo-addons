<odoo noupdate="1">
    <record model="ir.cron" id="cron_connector_lengow_import">
        <field name="name">Lengow Connector Imports</field>
        <field name="active" eval="False" />
        <field name="interval_number">10</field>
        <!-- Mandatory to define a explicit user on record from the frontend,
             DON'T USE admin (base.user_root) -->
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="state">code</field>
        <field name="model_id" ref="model_lengow_backend" />
        <field name="code">model._scheduler_import_sale_orders()</field>
    </record>
</odoo>
