<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.view.form.inherit.supplier.search</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='move_line_ids_without_package']/tree/field[@name='product_id']"
                position="attributes"
            >
                <attribute name="context">
                    {'picking_type_code': parent.picking_type_code, 'partner_id': parent.partner_id}
                </attribute>
            </xpath>
            <xpath
                expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']"
                position="attributes"
            >
                <attribute name="context">
                    {'picking_type_code': parent.picking_type_code, 'partner_id': parent.partner_id}
                </attribute>
            </xpath>
        </field>
    </record>
</odoo>
