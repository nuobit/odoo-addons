<odoo>
    <record id="account_asset_view_form" model="ir.ui.view">
        <field name="model">account.asset</field>
        <field
            name="inherit_id"
            ref="account_asset_management.account_asset_view_form"
        />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Prorate" name="prorate">
                    <group>
                        <group>
                            <field name="prorate_tax_id" />
                        </group>
                        <group>
                            <field name="tax_total_amount" />
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="temp_prorate_percent" />
                            <field name="temp_deductible_tax_amount" />
                            <field name="temp_non_deductible_tax_amount" />
                        </group>
                        <group>
                            <field name="final_prorate_percent" />
                            <field name="final_deductible_tax_amount" />
                            <field name="final_non_deductible_tax_amount" />
                        </group>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
