<odoo>
    <record
        id="anphitrion_pms_tiny_reservation_room_connector_form_view"
        model="ir.ui.view"
    >
        <field name="name">anphitrion.pms.tiny.reservation.room.form</field>
        <field name="model">anphitrion.pms.tiny.reservation.room</field>
        <field name="arch" type="xml">
            <form string="Anphitrion Reservation">
                <group>
                    <field name="backend_id" />
                    <field name="odoo_id" />
                    <field name="anphitrion_numreserva" />
                    <field name="anphitrion_linea" />
                    <field name="sync_date" readonly="True" />
                </group>
            </form>
        </field>
    </record>
    <record
        id="anphitrion_pms_tiny_reservation_room_connector_tree_view"
        model="ir.ui.view"
    >
        <field name="name">anphitrion.pms.tiny.reservation.room.tree</field>
        <field name="model">anphitrion.pms.tiny.reservation.room</field>
        <field name="arch" type="xml">
            <tree>
                <field name="backend_id" />
                <field name="odoo_id" />
                <field name="anphitrion_numreserva" />
                <field name="anphitrion_linea" />
                <field name="sync_date" />
            </tree>
        </field>
    </record>
</odoo>
