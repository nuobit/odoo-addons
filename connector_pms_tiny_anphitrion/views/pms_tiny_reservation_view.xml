<odoo>
    <record id="pms_tiny_reservation_form_view" model="ir.ui.view">
        <field name="name">pms.tiny.reservation.connector.form</field>
        <field name="model">pms.tiny.reservation</field>
        <field name="inherit_id" ref="pms_tiny.pms_tiny_reservation_form_view" />
        <field name="arch" type="xml">
            <xpath expr="/form/sheet" position="inside">
                <notebook>
                    <page string="Connectors" name="connector" invisible="0">
                        <group string="Anphitrion Bindings">
                            <field name="anphitrion_bind_ids" nolabel="1" />
                        </group>
                    </page>
                </notebook>
            </xpath>
        </field>
    </record>
</odoo>
