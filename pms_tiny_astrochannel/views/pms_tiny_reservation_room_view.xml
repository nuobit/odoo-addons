<odoo>
    <record id="pms_tiny_reservation_room_form_view" model="ir.ui.view">
        <field name="name">pms.tiny.reservation.room.form</field>
        <field name="model">pms.tiny.reservation.room</field>
        <field name="inherit_id" ref="pms_tiny.pms_tiny_reservation_room_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//sheet/group/group" position="after">
                <group>
                    <field name="confirm_number" />
                    <field name="confirm_time" />
                </group>
            </xpath>
        </field>
    </record>
</odoo>
