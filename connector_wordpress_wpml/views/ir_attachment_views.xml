<odoo>
    <record id="wordpress_ir_attachment_view_form" model="ir.ui.view">
        <field name="name">wordpress.ir.attachment.view.form</field>
        <field name="model">wordpress.ir.attachment</field>
        <field
            name="inherit_id"
            ref="connector_wordpress.wordpress_ir_attachment_view_form"
        />
        <field name="arch" type="xml">
            <field name="odoo_id" position="after">
                <field name="wordpress_lang" />
            </field>
        </field>
    </record>
   <record id="wordpress_ir_attachment_view_tree" model="ir.ui.view">
        <field name="name">wordpress.ir.attachment.view.tree</field>
        <field name="model">wordpress.ir.attachment</field>
        <field
            name="inherit_id"
            ref="connector_wordpress.wordpress_ir_attachment_view_tree"
        />
        <field name="arch" type="xml">
            <field name="odoo_id" position="after">
                <field name="wordpress_lang" />
            </field>
        </field>
    </record>
</odoo>
