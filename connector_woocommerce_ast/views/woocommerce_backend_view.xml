<odoo>
    <record id="woocommerce_ast_backend_view_form" model="ir.ui.view">
        <field name="name">woocommerce.ast.backend.form</field>
        <field name="model">woocommerce.backend</field>
         <field
            name="inherit_id"
            ref="connector_woocommerce.woocommerce_backend_view_form"
        />
        <field name="arch" type="xml">
            <group name="shipping" position="inside">
                     <field name="carrier_provider_ids" nolabel="1">
                    <tree editable="bottom">
                        <field name="delivery_type" />
                        <field name="woocommerce_provider" />
                    </tree>
                </field>
            </group>
        </field>
    </record>
</odoo>
